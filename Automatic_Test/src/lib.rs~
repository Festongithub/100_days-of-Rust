pub fn add(left: usize, right: usize) -> usize {
    left + right
}
pub fn sub(a: usize, b: usize) -> usize {
a - b
}

#[cfg(test)]
mod tests {
    use super::*;

    #[test]
    fn it_works() {
        let result = add(2, 2);
        assert_eq!(result, 4);
    }
    #[test]
    
    fn check_sub()
    {
    let diff = sub(561, 78);
    assert_eq!(diff, 483);
    }
}
